<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name ="Amazon Test Script" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="80.0"/>
    <parameter name ="url" value="http://amazon.com"></parameter>

    <test name = "Amazon Home">

        <classes>
            <class name="search.TestAllTheFeatures"></class>
<!--            <class name="search.TestSearch"></class>-->
<!--            <class name="search.TestPrime"></class>-->
<!--            <class name="search.DropDownMenu"></class>-->
        </classes>

    </test>

</suite>
